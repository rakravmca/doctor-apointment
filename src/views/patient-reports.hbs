

<!-- Page Heading -->
<div class="d-sm-flex align-items-center justify-content-between mb-4">
  <h1 class="h3 mb-0 text-gray-800">{{title}}</h1>
  <a href="#" class="btn btn-success btn-icon-split" onclick="generatePdf()"> 
            <span class="icon text-white-50">
                <i class="fas fa-check"></i>
            </span>
            <span class="text">Export PDF</span>
 </a>
</div>  

  <script>
function generatePdf(){
     ajaxCall.post('/reports/create', {reportType:'patient-appointments'},
            success = function(data, textStatus, jqXHR)
            {
                window.open('/output/' + data.fileName, "_blank"); 
                //document.location.href = '/output/' + data.fileName;
            },
            error = function(data, textStatus, jqXHR){
                console.log(textStatus + ': ' + jqXHR);
            }
        );
}
  </script>